<?xml version="1.0" encoding="utf-8"?>
<project name="DummyResourceAdaptor" default="build-and-deploy-ra">
	
	<property name="mobicents.home" value="${ant.file.DummyResourceAdaptor}/../../.." />	
	<import file="${mobicents.home}/../ant-scripts/mobicents-ant-common-ra.xml" />
	
	<property name="ra.name" value="dummy" />
	<property name="ra.entity" value="DummyRA" />
	<property name="ra.id" value="${ant.project.name}#org.mobicents#1.0" />	
	<property name="ra.entity.properties" value="dummy-ra.properties"/>
	<path id="ra.classpath">
		<path refid="mobicents.core.classpath" />
		<pathelement location="${ant.file.DummyResourceAdaptor}/../lib/sleetck-minimal.jar" />
		<pathelement location="${ant.file.DummyResourceAdaptor}/../ratype/jars/dummy-ratype.jar" />
	</path>

	<fileset id="ra.libs.ratype" dir="lib">
		<include name="sleetck-minimal.jar" />
	</fileset>
	
</project>


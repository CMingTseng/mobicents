<?xml version="1.0" encoding="utf-8"?>
<project name="ClickToDialWar" default="deploy">

	<property name="mobicents.home" value="../../../mobicents" />

	<import file="${mobicents.home}/xml/ant/mobicents-ant-common-properties.xml" />
	<import file="${mobicents.home}/xml/ant/mobicents-ant-common-service.xml" />

	<property name="war.dir" value="httpservletra.war" />
        <property name="war.deploy.dir" value="${jboss.home}/server/all/deploy" />

	<target name="deploy" >
		  <copy todir="${war.deploy.dir}/${war.dir}">
		   	<fileset dir="${war.dir}"/>
		  </copy>
	</target>

	<target name="undeploy" >
		<delete dir="${war.deploy.dir}/${war.dir}" />
	</target>

	<target name="clean" depends="undeploy" />

</project>


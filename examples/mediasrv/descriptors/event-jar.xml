<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE event-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Event 1.0//EN"
"http://java.sun.com/dtd/slee-event-jar_1_0.dtd">

<event-jar>
    <event-definition>
        <description>
            This event is fired when dialog started.
        </description>
        <event-type-name>org.mobicents.examples.media.DIALOG_STARTED</event-type-name>
        <event-type-vendor>org.mobicents.media</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.examples.media.events.DialogStartedEvent</event-class-name>
    </event-definition>
    
    <event-definition>
        <description>
            This event is fired when dialog completed.
        </description>
        <event-type-name>org.mobicents.examples.media.DIALOG_COMPLETED</event-type-name>
        <event-type-vendor>org.mobicents.media</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.examples.media.events.DialogCompletedEvent</event-class-name>
    </event-definition>    
</event-jar>

<project basedir="." default="" name="Registrar">

	<!-- OTHER TARGET THAN build-sbb, and clean should not be called -->
	<property environment="sys" />
	<property name="jboss.home" value="${system.JBOSS_HOME}" />

	<!-- WE NEED TO DEFINE THOSE -->
	<property name="service.name" value="registrar" />
	<property name="service.id" value="SIP Registrar Service#mobicents#1.1" />
	<property name="service.src.path" value="org/mobicents/slee/services/sip/registrar" />
	<property name="service.inludes.src.path" value="${service.src.path}/**/*.java org/mobicents/slee/services/sip/common/**/*.java"/>


	<path id="service.classpath">
		<pathelement location="../lib/jain-sip-ri-1.2.1.jar" />
		<pathelement location="../lib/jain-sip-api-1.2.1.jar" />
		<path refid="mobicents.core.classpath"/>
		<pathelement location="../ratype/jars/sip-ratype.jar" />
		<pathelement location="../ra/jars/sip-ra.jar" />
		<pathelement location="./jars/sipservices_common.jar" />
		<pathelement location="${jboss.home}/lib/jboss-system.jar" />
		<pathelement location="${jboss.home}/client/jbossall-client.jar" />
	</path>


	<property name="compile.as.debug" value="false" />

	<fileset id="service.other.jars" file="." />


	<import file="${ant.file.Registrar}/../../../../core/xml/ant/mobicents-ant-common-service.xml" />










</project>

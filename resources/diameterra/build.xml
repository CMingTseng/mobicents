<?xml version="1.0" encoding="utf-8"?>
<project name="DiameterResourceAdaptor" default="build-and-deploy-ra">

	
	<import file="$${ant.file.DiameterResourceAdaptor}/../../../core/xml/ant/mobicents-ant-common-ra.xml" />

	<property name="ra.name" value="diameter" />
	<property name="ra.entity" value="DiameterRA" />
	<property name="ra.id" value="${ant.project.name}#org.mobicents#1.0" />	
	
	<path id="ra.classpath">
		<path refid="mobicents.core.classpath" />
		<path refid="mobicents.ra.${ra.name}ra.classpath" />
	</path>

	<fileset id="ra.libs.ratype" dir="lib">
		<include name="Diameter.jar" />
	</fileset>
	
</project>


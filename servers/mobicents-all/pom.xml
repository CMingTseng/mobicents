	<modelVersion>4.0.0</modelVersion>
	<packaging>pom</packaging>

	<artifactId>mobicents-all-servers-checkout</artifactId>
	<groupId>org.mobicents</groupId>
	<version>1.0</version>
	
	<properties>
		<checkout.basedir>${basedir}/target/servers/</checkout.basedir>
		<checkout.username>mobicents-read-only</checkout.username>
		<checkout.password></checkout.password>
		
		<jainslee.connectionURL>scm:svn:http://mobicents.googlecode.com/svn/tags/mobicents-jainslee-server-1.2.0.CR1</jainslee.connectionURL>
		<media.core.connectionURL>scm:svn:http://mobicents.googlecode.com/svn/tags/mobicents-media-server-core-1.0.0.BETA3</media.core.connectionURL>

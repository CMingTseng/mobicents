<?xml version="1.0" encoding="utf-8"?>
<!-- !DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.0//EN"
                         "http://java.sun.com/dtd/slee-sbb-jar_1_0.dtd" -->
<sbb-jar>
    <sbb>
        <description/>
        <sbb-name>InstallServiceTestSbb</sbb-name>
        <sbb-vendor>mobicents</sbb-vendor>
        <sbb-version>0.1</sbb-version>
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>org.mobicents.sleetests.container.installService.InstallServiceTestSbb</sbb-abstract-class-name>
                <cmp-field>
					<description>Service ID for which this controller works. FORMAT: ServiceID[xxx#xx#x]</description>
					<cmp-field-name>serviceID</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<description>Service component, see descriptiotn of ServiceID</description>
					<cmp-field-name>svc</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<description>component key of implemented ra type</description>
					<cmp-field-name>raTypeKey</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<description>component key for ra</description>
					<cmp-field-name>raKey</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<description></description>
					<cmp-field-name>x1_eventIDsPassedToRABySleeContainer</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<description></description>
					<cmp-field-name>x1_optionsPassedToRABySleeContainer</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<description></description>
					<cmp-field-name>x1_eventComponentKeysToServiceIdSets_RA_STATE</cmp-field-name>
				</cmp-field>
				 <!-- <cmp-field>
					<description></description>
					<cmp-field-name>x1_futureSetOfResourceOptions</cmp-field-name>
				</cmp-field> -->
				
				<cmp-field>
					<description></description>
					<cmp-field-name>x1_eventComponentKeysThatAreNotInRaDesc</cmp-field-name>
				</cmp-field>
				
				<cmp-field>
					<description></description>
					<cmp-field-name>x1_areAllEventIDsPassedToRADecalredByService</cmp-field-name>
				</cmp-field>
				
				<cmp-field>
					<description></description>
					<cmp-field-name>x1_haveEventIDPassedbySleeContainerBeenDoubled</cmp-field-name>
				</cmp-field>
				
				<cmp-field>
					<description></description>
					<cmp-field-name>x1_areAllPassedEventIDsInRaTypeDesc</cmp-field-name>
				</cmp-field>
				<cmp-field>
					<description></description>
					<cmp-field-name>endMessage</cmp-field-name>
				</cmp-field>
            </sbb-abstract-class>
        </sbb-classes>
        
        <event event-direction="Receive" initial-event="False" mask-on-attach="False">
			<event-name>TimeEvent</event-name>
			<event-type-ref>
				<event-type-name>javax.slee.facilities.TimerEvent</event-type-name>
				<event-type-vendor>javax.slee</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			
		</event>
        
        <event event-direction="Receive" initial-event="True">
			<event-name>StartServiceEvent</event-name>
			<event-type-ref>
				<event-type-name>javax.slee.serviceactivity.ServiceStartedEvent</event-type-name>
				<event-type-vendor>javax.slee</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>
		
		
		
		<event event-direction="Receive" initial-event="True">
			<event-name>TCKResourceEventX1</event-name>
			<event-type-ref>
				<event-type-name>com.opencloud.sleetck.lib.resource.events.TCKResourceEventX.X1</event-type-name>
				<event-type-vendor>jain.slee.tck</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<initial-event-selector-method-name>chooseService</initial-event-selector-method-name>
		</event>
		
		
		
		
       <resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>TCK_Resource_Type</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>jain.slee.tck</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>1.0</resource-adaptor-type-version>
			</resource-adaptor-type-ref>
			<activity-context-interface-factory-name>slee/resources/tck/activitycontextinterfacefactory</activity-context-interface-factory-name>
			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>slee/resources/tck/resource</resource-adaptor-object-name>
				<resource-adaptor-entity-link>slee/resources/tck</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>
		</resource-adaptor-type-binding>
        
    </sbb>
</sbb-jar>

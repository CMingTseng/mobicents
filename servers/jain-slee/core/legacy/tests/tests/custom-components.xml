<?xml version="1.0"?>

<!-- This file contains calls to build files for SLEE components defined within the SLEE TCK test suite -->
<project name="JAIN SLEE TCK -- custom components" default="assemble-custom-components">

   <!-- Assembles custom components. Delegates to assemble-custom-components-* targets -->
   <target name="assemble-custom-components">

     <subant target="" inheritall="yes">
         <property name="xyz.build.dir" value="subant1.build"/>
         <property name="xyz.not.overloaded" value="not.overloaded"/>
         <fileset dir="${build.dir}/src" includes="**/custom-components.xml"/>
     </subant>
   </target>     
</project>


<?xml version="1.0"?>

<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.0//EN" "http://java.sun.com/dtd/slee-sbb-jar_1_0.dtd">

<sbb-jar>
	<sbb id="callsbb">
		<description>Represents the entire call</description>
		<sbb-name>MGCPDemoCallSbb</sbb-name>
		<sbb-vendor>org.mobicents</sbb-vendor>
		<sbb-version>1.0</sbb-version>
		<sbb-alias>MGCPDemoCallSbb</sbb-alias>

		<sbb-ref>
			<sbb-name>CRCXSbb</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>CRCXSbb</sbb-alias>
		</sbb-ref>

		<sbb-ref>
			<sbb-name>CRCXEndpointSbb</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>CRCXEndpointSbb</sbb-alias>
		</sbb-ref>

		<sbb-ref>
			<sbb-name>MDCXSbb</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>MDCXSbb</sbb-alias>
		</sbb-ref>
		
		<sbb-ref>
			<sbb-name>RQNTSbb</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>RQNTSbb</sbb-alias>
		</sbb-ref>		

		<sbb-classes>
			<sbb-abstract-class>
				<sbb-abstract-class-name>
					org.mobicents.mgcp.demo.CallSbb
				</sbb-abstract-class-name>
				<get-child-relation-method>
					<sbb-alias-ref>CRCXSbb</sbb-alias-ref>
					<get-child-relation-method-name>
						getCRCXSbbChild
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>
				<get-child-relation-method>
					<sbb-alias-ref>CRCXEndpointSbb</sbb-alias-ref>
					<get-child-relation-method-name>
						getCRCXEndpointSbbChild
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>
				<get-child-relation-method>
					<sbb-alias-ref>MDCXSbb</sbb-alias-ref>
					<get-child-relation-method-name>
						getMDCXSbbChild
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>
				<get-child-relation-method>
					<sbb-alias-ref>RQNTSbb</sbb-alias-ref>
					<get-child-relation-method-name>
						getRQNTSbbChild
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>				
			</sbb-abstract-class>
		</sbb-classes>

		<event event-direction="Receive" initial-event="True">
			<event-name>Invite</event-name>
			<event-type-ref>
				<event-type-name>
					javax.sip.message.Request.INVITE
				</event-type-name>
				<event-type-vendor>javax.sip</event-type-vendor>
				<event-type-version>1.1</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>
					JAIN SIP
				</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>
					javax.sip
				</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>
					1.2
				</resource-adaptor-type-version>
			</resource-adaptor-type-ref>
			<activity-context-interface-factory-name>
				slee/resources/jainsip/1.2/acifactory
			</activity-context-interface-factory-name>
			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					slee/resources/jainsip/1.2/provider
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					SipRA
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>
		</resource-adaptor-type-binding>
	</sbb>
</sbb-jar>

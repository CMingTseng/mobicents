<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.0//EN"
                         "http://java.sun.com/dtd/slee-sbb-jar_1_0.dtd">
<sbb-jar>
    <sbb>
        <description>Call Controller for Call Forward and Block services</description>
        <sbb-name>CallController</sbb-name>
        <sbb-vendor>Mobicents</sbb-vendor>
        <sbb-version>1.0</sbb-version>
        <sbb-ref>
            <sbb-name>CallBlockSbb</sbb-name>
            <sbb-vendor>Mobicents</sbb-vendor>
            <sbb-version>1.0</sbb-version>
            <sbb-alias>CallBlockSbb</sbb-alias>
        </sbb-ref>
        <sbb-ref>
            <sbb-name>CallForward Sbb</sbb-name>
            <sbb-vendor>Mobicents</sbb-vendor>
            <sbb-version>1.0</sbb-version>
            <sbb-alias>CallForwardSbb</sbb-alias>
        </sbb-ref>
        <sbb-ref>
            <sbb-name>Bye SBB</sbb-name>
            <sbb-vendor>Mobicents</sbb-vendor>
            <sbb-version>1.0</sbb-version>
            <sbb-alias>ByeSbb</sbb-alias>
        </sbb-ref>
        <sbb-ref>
            <sbb-name>SipRegistrarSbb</sbb-name>
            <sbb-vendor>mobicents</sbb-vendor>
            <sbb-version>1.0</sbb-version>
            <sbb-alias>RegistrarSbb</sbb-alias>
        </sbb-ref>
        <sbb-ref>
            <sbb-name>ProxySbb</sbb-name>
            <sbb-vendor>mobicents</sbb-vendor>
            <sbb-version>1.0</sbb-version>
            <sbb-alias>ProxySbb</sbb-alias>
        </sbb-ref>
        <profile-spec-ref>
            <profile-spec-name>CallForwardBlock Profile Spec</profile-spec-name>
            <profile-spec-vendor>Mobicents</profile-spec-vendor>
            <profile-spec-version>1.0</profile-spec-version>
            <profile-spec-alias>CallForwardBlockProfile</profile-spec-alias>
        </profile-spec-ref>
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>org.mobicents.slee.examples.callforwardblock.controller.ControllerSbb</sbb-abstract-class-name>
                <get-child-relation-method>
                    <sbb-alias-ref>CallBlockSbb</sbb-alias-ref>
                    <get-child-relation-method-name>getCallBlockSbbChild</get-child-relation-method-name>
                    <default-priority>0</default-priority>
                </get-child-relation-method>
                <get-child-relation-method>
                    <sbb-alias-ref>CallForwardSbb</sbb-alias-ref>
                    <get-child-relation-method-name>getCallForwardSbbChild</get-child-relation-method-name>
                    <default-priority>0</default-priority>
                </get-child-relation-method>
                <get-child-relation-method>
                    <sbb-alias-ref>ByeSbb</sbb-alias-ref>
                    <get-child-relation-method-name>getByeSbbChild</get-child-relation-method-name>
                    <default-priority>0</default-priority>
                </get-child-relation-method>
                <get-child-relation-method>
                    <sbb-alias-ref>RegistrarSbb</sbb-alias-ref>
                    <get-child-relation-method-name>getRegistrarSbbChild</get-child-relation-method-name>
                    <default-priority>0</default-priority>
                </get-child-relation-method>
                <get-child-relation-method>
                    <sbb-alias-ref>ProxySbb</sbb-alias-ref>
                    <get-child-relation-method-name>getProxySbbChild</get-child-relation-method-name>
                    <default-priority>0</default-priority>
                </get-child-relation-method>
                <get-profile-cmp-method>
                    <profile-spec-alias-ref>CallForwardBlockProfile</profile-spec-alias-ref>
                    <get-profile-cmp-method-name>getCallForwardBlockProfileCMP</get-profile-cmp-method-name>
                </get-profile-cmp-method>
            </sbb-abstract-class>
			<sbb-activity-context-interface>
				<sbb-activity-context-interface-name>
					org.mobicents.slee.examples.callforwardblock.controller.ControllerActivityContextInterface
				</sbb-activity-context-interface-name>
			</sbb-activity-context-interface>
        </sbb-classes>
        <event event-direction="Fire" initial-event="False">
            <event-name>CallBlockReqEvent</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.examples.callforwardblock.events.CallBlockReqEvent</event-type-name>
                <event-type-vendor>Mobicents</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>
        <event event-direction="Receive" initial-event="True">
            <event-name>RegisterEvent</event-name>
            <event-type-ref>
                <event-type-name>javax.sip.message.Request.REGISTER</event-type-name>
                <event-type-vendor>javax.sip</event-type-vendor>
                <event-type-version>1.1</event-type-version>
            </event-type-ref>
            <initial-event-select variable="ActivityContext"/>
        </event>
        <event event-direction="Receive" initial-event="True">
            <event-name>InviteEvent</event-name>
            <event-type-ref>
                <event-type-name>javax.sip.message.Request.INVITE</event-type-name>
                <event-type-vendor>javax.sip</event-type-vendor>
                <event-type-version>1.1</event-type-version>
            </event-type-ref>
	    	<initial-event-selector-method-name>callIDSelect</initial-event-selector-method-name>
        </event>
        <event event-direction="Receive" initial-event="True">
            <event-name>AckEvent</event-name>
            <event-type-ref>
                <event-type-name>javax.sip.message.Request.ACK</event-type-name>
                <event-type-vendor>javax.sip</event-type-vendor>
                <event-type-version>1.1</event-type-version>
            </event-type-ref>
	    	<initial-event-selector-method-name>callIDSelect</initial-event-selector-method-name>
        </event>
        <event event-direction="Receive" initial-event="True">
            <event-name>ByeEvent</event-name>
            <event-type-ref>
                <event-type-name>javax.sip.message.Request.BYE</event-type-name>
                <event-type-vendor>javax.sip</event-type-vendor>
                <event-type-version>1.1</event-type-version>
            </event-type-ref>
	    	<initial-event-selector-method-name>callIDSelect</initial-event-selector-method-name>
        </event>
        <event event-direction="Fire" initial-event="False">
            <event-name>CallForwardEvent</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.examples.callforwardblock.events.CallForwardEvent</event-type-name>
                <event-type-vendor>Mobicents</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>
        <event event-direction="Receive" initial-event="False">
            <event-name>CallBlockSuccessEvent</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.examples.callforwardblock.events.CallBlockSuccessEvent</event-type-name>
                <event-type-vendor>Mobicents</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>
        <event event-direction="Receive" initial-event="False">
            <event-name>CallForwardFailureEvent</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.examples.callforwardblock.events.CallForwardFailureEvent</event-type-name>
                <event-type-vendor>Mobicents</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>
        <event event-direction="Receive" initial-event="False">
            <event-name>CallForwardSuccessEvent</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.examples.callforwardblock.events.CallForwardSuccessEvent</event-type-name>
                <event-type-vendor>Mobicents</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>
        <resource-adaptor-type-binding>
            <resource-adaptor-type-ref>
                <resource-adaptor-type-name>jain-sip</resource-adaptor-type-name>
                <resource-adaptor-type-vendor>javax.sip</resource-adaptor-type-vendor>
                <resource-adaptor-type-version>1.1</resource-adaptor-type-version>
            </resource-adaptor-type-ref>
            <activity-context-interface-factory-name>slee/resources/jainsip/1.1/acifactory</activity-context-interface-factory-name>
            <resource-adaptor-entity-binding>
                <resource-adaptor-object-name>slee/resources/jainsip/1.1/provider</resource-adaptor-object-name>
                <resource-adaptor-entity-link>SipRA</resource-adaptor-entity-link>
            </resource-adaptor-entity-binding>
        </resource-adaptor-type-binding>
    </sbb>
</sbb-jar>

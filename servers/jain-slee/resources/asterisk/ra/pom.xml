<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">	
	<modelVersion>4.0.0</modelVersion>
	
	<parent>
		<artifactId>asterisk-parent</artifactId>
		<groupId>org.mobicents.resources</groupId>
		<version>1.3.0-SNAPSHOT</version>
	</parent>
	
	<artifactId>asterisk-ra</artifactId>
	<name>Mobicents :: Resources :: ${pom.artifactId}</name>

	<properties>
        	<!-- properties to activate ra -->
        	<MAGIIP>192.168.6.128</MAGIIP>
        	<MAGILogin>admin</MAGILogin>
        	<MAGIPassword>amp111</MAGIPassword>
        	<responseTimeout>300000</responseTimeout>
	</properties>
	
	<dependencies>	
		<dependency>
			<groupId>${pom.groupId}</groupId>
			<artifactId>asterisk-ratype</artifactId>
			<version>${pom.version}</version>
		</dependency>
		<dependency>
			<groupId>org.mobicents.core</groupId>
			<artifactId>mobicents-core-jar</artifactId>
			<version>${pom.version}</version>
		</dependency>
	</dependencies>

	<build>
		<resources>
			<resource>
				<directory>${basedir}/src/main/resources</directory>
				<filtering>true</filtering>
				<includes>
					<include>**/*.properties</include>
					<include>**/*.xml</include>
				</includes>
			</resource>
		</resources>
	</build>		
</project>
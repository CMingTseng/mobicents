<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.0//EN"
                         "http://java.sun.com/dtd/slee-sbb-jar_1_0.dtd">
<sbb-jar>
    <sbb>
        <description>This SBB has to process the INVITE to know whether
            it is blocked</description>
        <sbb-name>CallBlockingSbb</sbb-name>
        <sbb-vendor>org.mobicents</sbb-vendor>
        <sbb-version>0.1</sbb-version>
        <profile-spec-ref>
            <profile-spec-name>ControllerProfileCMP</profile-spec-name>
            <profile-spec-vendor>org.mobicents</profile-spec-vendor>
            <profile-spec-version>0.1</profile-spec-version>
            <profile-spec-alias>ControllerProfile</profile-spec-alias>
        </profile-spec-ref>
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>org.mobicents.slee.examples.blocking.CallBlockingSbb</sbb-abstract-class-name>
                <get-profile-cmp-method>
                    <profile-spec-alias-ref>ControllerProfile</profile-spec-alias-ref>
                    <get-profile-cmp-method-name>getControllerProfileCMP</get-profile-cmp-method-name>
                </get-profile-cmp-method>
            </sbb-abstract-class>
            <sbb-local-interface>
                <sbb-local-interface-name>org.mobicents.slee.examples.blocking.CallBlockingSbbLocalObject</sbb-local-interface-name>
            </sbb-local-interface>
        </sbb-classes>
        <resource-adaptor-type-binding>
            <resource-adaptor-type-ref>
                <resource-adaptor-type-name>JAIN SIP</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>javax.sip</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>1.2</resource-adaptor-type-version>
            </resource-adaptor-type-ref>
            <activity-context-interface-factory-name>slee/resources/jainsip/1.2/acifactory</activity-context-interface-factory-name>
            <resource-adaptor-entity-binding>
                <resource-adaptor-object-name>slee/resources/jainsip/1.2/provider</resource-adaptor-object-name>
                <resource-adaptor-entity-link>SipRA</resource-adaptor-entity-link>
            </resource-adaptor-entity-binding>
        </resource-adaptor-type-binding>
    </sbb>
</sbb-jar>

<!-- Delegating build script, used by cruisecontrol to build Mobicents SLEE.
     Note that the basedir is set to the checked out project -->
<project name="build-mobicents-dailytckrun" default="build" basedir="checkout/mobicents24">
	<target name="build">
		<!-- Get the latest from CVS -->
		<cvs command="up -d -P"/>
		<!-- Call the target that does everything -->
		<ant antfile="build.xml" target="cc-dailytckrun"/> 
		<!-- ant antfile="build.xml" target="cc-build-loop"/ -->
	</target>
</project>

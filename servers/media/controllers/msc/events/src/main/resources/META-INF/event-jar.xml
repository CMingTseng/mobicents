<event-jar>
	<event-definition>
		<description>
			This event is fired when DTMF is received.
		</description>
		<event-type-name>org.mobicents.slee.media.DTMF</event-type-name>
		<event-type-vendor>org.mobicents.media</event-type-vendor>
		<event-type-version>1.0</event-type-version>
		<event-class-name>
			org.mobicents.slee.resource.media.events.DtmfEvent
		</event-class-name>
	</event-definition>
	<event-definition>
		<description>
			This event is fired to know whether the creation of a
			Transmitter, Receiver or Transmitter-Receiver was a success
			or not.
		</description>
		<event-type-name>
			org.mobicents.slee.media.SessionResult
		</event-type-name>
		<event-type-vendor>org.mobicents.media</event-type-vendor>
		<event-type-version>1.0</event-type-version>
		<event-class-name>
			org.mobicents.slee.resource.media.events.SessionResultEvent
		</event-class-name>
	</event-definition>


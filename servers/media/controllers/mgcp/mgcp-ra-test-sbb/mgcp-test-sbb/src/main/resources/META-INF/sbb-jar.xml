<?xml version="1.0"?>

<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.0//EN" "http://java.sun.com/dtd/slee-sbb-jar_1_0.dtd">

<sbb-jar>
	<sbb id="mgcp">
		<description>MGCP RA Test</description>
		<sbb-name>MgcpRaTestSbb</sbb-name>
		<sbb-vendor>org.mobicents</sbb-vendor>
		<sbb-version>1.0</sbb-version>
		<sbb-alias>MgcpRaTestSbb</sbb-alias>



		<sbb-classes>
			<sbb-abstract-class>
				<sbb-abstract-class-name>
					org.mobicents.media.server.control.mgcp.ra.test.MgcpRaTestSbb
				</sbb-abstract-class-name>
				<cmp-field>
					<cmp-field-name>completedMgcpCycle</cmp-field-name>
				</cmp-field>
			</sbb-abstract-class>
		</sbb-classes>

		<event event-direction="Receive" initial-event="True">
			<event-name>CreateConnection</event-name>
			<event-type-ref>
				<event-type-name>
					net.java.slee.resource.mgcp.CREATE_CONNECTION
				</event-type-name>
				<event-type-vendor>net.java</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<event event-direction="Receive" initial-event="False">
			<event-name>DeleteConnection</event-name>
			<event-type-ref>
				<event-type-name>
					net.java.slee.resource.mgcp.DELETE_CONNECTION
				</event-type-name>
				<event-type-vendor>net.java</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<!-- initial-event-select variable="ActivityContext" / -->
		</event>

		<event event-direction="Receive" initial-event="False">
			<event-name>ModifyConnection</event-name>
			<event-type-ref>
				<event-type-name>
					net.java.slee.resource.mgcp.MODIFY_CONNECTION
				</event-type-name>
				<event-type-vendor>net.java</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<!--  initial-event-select variable="ActivityContext" / -->
		</event>

		<event event-direction="Receive" initial-event="False">
			<event-name>NotificationRequest</event-name>
			<event-type-ref>
				<event-type-name>
					net.java.slee.resource.mgcp.NOTIFICATION_REQUEST
				</event-type-name>
				<event-type-vendor>net.java</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<!-- initial-event-selector-method-name>
				endpointIdSelect
			</initial-event-selector-method-name -->
		</event>

		<resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>
					jain-mgcp
				</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>
					net.java
				</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>
					2.0
				</resource-adaptor-type-version>
			</resource-adaptor-type-ref>
			<activity-context-interface-factory-name>
				slee/resources/jainmgcp/2.0/acifactory
			</activity-context-interface-factory-name>
			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					slee/resources/jainmgcp/2.0/provider
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					MGCP-MRF
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>
		</resource-adaptor-type-binding>
	</sbb>
</sbb-jar>

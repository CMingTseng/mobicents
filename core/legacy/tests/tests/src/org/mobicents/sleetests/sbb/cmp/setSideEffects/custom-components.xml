<?xml version="1.0"?>

<project name="JAIN SLEE Custom Tests" default="package">
  <property name="test.dir" value="/org/mobicents/sleetests/sbb/cmp/setSideEffects"/>
  <property name="test.src" value="${tck.source}/${test.dir}"/>
  <target name="package">
    <echo>classes.sleetests=${classes.sleetests}</echo>
    <echo>test.src=${test.src}</echo>
    <echo>jars.deployable-units=${jars.deployable-units}</echo>
    <deployablejar destfile="${jars.deployable-units}/org.mobicents.sleetests.sbb.cmp.setSideEffects.CMPTest-du.jar" 
      servicexml="${test.src}/META-INF/CMPTest-service-jar.xml">
      <sbbjar sbbjarxml="${test.src}/META-INF/CMPTestRoot-sbb-jar.xml" 
        classpath="${classes.sleetests}"/>
      <sbbjar sbbjarxml="${test.src}/META-INF/CMPTestChild-sbb-jar.xml"
        classpath="${classes.sleetests}"/>
    </deployablejar>
  </target>
</project>

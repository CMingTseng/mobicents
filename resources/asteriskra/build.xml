<?xml version="1.0" encoding="utf-8"?>
<project name="AsteriskResourceAdaptor" default="build-and-deploy-ra">

	<property name="mobicents.home" value="../.." />	
	<!--import file="${mobicents.home}/xml/ant/mobicents-ant-common-properties.xml" /-->
	<import file="${mobicents.home}/xml/ant/mobicents-ant-common-ra.xml" />

	<property name="ra.name" value="asterisk" />
	<property name="ra.entity" value="AsteriskRA" />
	<property name="ra.id" value="${ant.project.name}#net.sf.asterisk#1.0" />	
	
	<path id="ra.classpath">
		<path refid="mobicents.core.classpath" />
		<path refid="mobicents.ra.asteriskra.classpath" />
	</path>

	<fileset id="ra.libs.ratype" dir="lib">
		<include name="asterisk-java.jar" />
	</fileset>
	
</project>


<?xml version="1.0" encoding="utf-8"?>
<project name="HttpClientResourceAdaptor" default="build-and-deploy-ra">



	<import file="$${ant.file.HttpClientResourceAdaptor}/../../../core/xml/ant/mobicents-ant-common-ra.xml" />


	<property name="ra.name" value="httpclient" />
	<property name="ra.entity" value="HttpClientRA" />
	<property name="ra.id" value="${ant.project.name}#org.mobicents#1.0" />

	<path id="ra.classpath">
		<path refid="mobicents.core.classpath" />
		<path refid="mobicents.ra.httpclientra.classpath" />
	</path>

	<fileset id="ra.libs.ratype" dir="lib">
		<include name="commons-codec-1.3.jar" />
		<include name="commons-httpclient-3.0.1.jar" />
	</fileset>

</project>


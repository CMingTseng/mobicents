<?xml version="1.0"?>

<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.0//EN" "http://java.sun.com/dtd/slee-sbb-jar_1_0.dtd">

<sbb-jar>            
    <sbb id="callsbb">
        <description>Represents the entire call</description>
        <sbb-name>CallSbb</sbb-name>
        <sbb-vendor>org.mobicents</sbb-vendor>
        <sbb-version>1.0</sbb-version>
        <sbb-alias>CallSbb</sbb-alias>
        
        <sbb-ref>
            <sbb-name>LoopDemoSbb</sbb-name>
            <sbb-vendor>org.mobicents</sbb-vendor>
            <sbb-version>1.0</sbb-version>
            <sbb-alias>LoopDemoSbb</sbb-alias>
        </sbb-ref>
        
        
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>
                    org.mobicents.examples.media.CallSbb
                </sbb-abstract-class-name>
                <cmp-field>
                    <cmp-field-name>destination</cmp-field-name>
                </cmp-field>
                <get-child-relation-method>
                    <sbb-alias-ref>LoopDemoSbb</sbb-alias-ref>
                    <get-child-relation-method-name>getLoopDemoSbb</get-child-relation-method-name>
                    <default-priority>0</default-priority>
                </get-child-relation-method>
            </sbb-abstract-class>                                               
        </sbb-classes>
        
        <event event-direction="Receive" initial-event="True">
            <event-name>CallCreated</event-name>
            <event-type-ref>
                <event-type-name>javax.sip.message.Request.INVITE</event-type-name>
                <event-type-vendor>javax.sip</event-type-vendor>
                <event-type-version>1.1</event-type-version>
            </event-type-ref>
            <initial-event-select variable="ActivityContext"/>
        </event>
        <event event-direction="Receive" initial-event="False">
            <event-name>CallTerminated</event-name>
            <event-type-ref>
                <event-type-name>javax.sip.dialog.TerminationRequest</event-type-name>
                <event-type-vendor>net.java</event-type-vendor>
                <event-type-version>1.2</event-type-version>
            </event-type-ref>
        </event>
        
        <event event-direction="Receive" initial-event="False">
            <event-name>ConnectionCreated</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.media.CONNECTION_CREATED</event-type-name>
                <event-type-vendor>org.mobicents.media</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>
        
        <resource-adaptor-type-binding>
            <resource-adaptor-type-ref>
                <resource-adaptor-type-name>JAIN SIP</resource-adaptor-type-name>
                <resource-adaptor-type-vendor>javax.sip</resource-adaptor-type-vendor>
                <resource-adaptor-type-version>1.2</resource-adaptor-type-version>
            </resource-adaptor-type-ref>
            <activity-context-interface-factory-name>slee/resources/jainsip/1.2/acifactory</activity-context-interface-factory-name>
            <resource-adaptor-entity-binding>
                <resource-adaptor-object-name>slee/resources/jainsip/1.2/provider</resource-adaptor-object-name>
                <resource-adaptor-entity-link>SipRA</resource-adaptor-entity-link>
            </resource-adaptor-entity-binding>
        </resource-adaptor-type-binding>
        <resource-adaptor-type-binding>
            <resource-adaptor-type-ref>
                <resource-adaptor-type-name>media_ratype</resource-adaptor-type-name>
                <resource-adaptor-type-vendor>org.mobicents.media</resource-adaptor-type-vendor>
                <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
            </resource-adaptor-type-ref>
            <activity-context-interface-factory-name>slee/resources/media/1.0/acifactory</activity-context-interface-factory-name>
            <resource-adaptor-entity-binding>
                <resource-adaptor-object-name>slee/resources/media/1.0/provider</resource-adaptor-object-name>
                <resource-adaptor-entity-link>LocalMs</resource-adaptor-entity-link>
            </resource-adaptor-entity-binding>
        </resource-adaptor-type-binding>				        
        
    </sbb>  
    
    <sbb id="LoopDemoSbb">
        <description>Represents the loopback example</description>
        <sbb-name>LoopDemoSbb</sbb-name>
        <sbb-vendor>org.mobicents</sbb-vendor>
        <sbb-version>1.0</sbb-version>
        <sbb-alias>LoopDemoSbb</sbb-alias>
        
        <sbb-ref>
            <sbb-name>AnnouncementSbb</sbb-name>
            <sbb-vendor>org.mobicents</sbb-vendor>
            <sbb-version>1.0</sbb-version>
            <sbb-alias>AnnouncementSbb</sbb-alias>
        </sbb-ref>
        
        <sbb-ref>
            <sbb-name>LoopbackSbb</sbb-name>
            <sbb-vendor>org.mobicents</sbb-vendor>
            <sbb-version>1.0</sbb-version>
            <sbb-alias>LoopbackSbb</sbb-alias>
        </sbb-ref>
        
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>
                    org.mobicents.examples.media.loop.LoopDemoSbb
                </sbb-abstract-class-name>  
                <cmp-field>
                    <cmp-field-name>userEndpoint</cmp-field-name>
                </cmp-field>
                <get-child-relation-method>
                    <sbb-alias-ref>AnnouncementSbb</sbb-alias-ref>
                    <get-child-relation-method-name>getAnnouncementSbb</get-child-relation-method-name>
                    <default-priority>0</default-priority>
                </get-child-relation-method>
                <get-child-relation-method>
                    <sbb-alias-ref>LoopbackSbb</sbb-alias-ref>
                    <get-child-relation-method-name>getLoopbackSbb</get-child-relation-method-name>
                    <default-priority>0</default-priority>
                </get-child-relation-method>
            </sbb-abstract-class> 
            <sbb-local-interface>
                <sbb-local-interface-name>
                    org.mobicents.examples.media.Demo
                </sbb-local-interface-name>
            </sbb-local-interface>
        </sbb-classes>
        
        <event event-direction="Receive" initial-event="False">
            <event-name>AnnouncementComplete</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.media.LINK_DROPPED</event-type-name>
                <event-type-vendor>org.mobicents.media</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>    
    </sbb>  
    
    <sbb id="AnnouncementSbb">
        <description>Represents the Announcement</description>
        <sbb-name>AnnouncementSbb</sbb-name>
        <sbb-vendor>org.mobicents</sbb-vendor>
        <sbb-version>1.0</sbb-version>
        <sbb-alias>AnnouncementSbb</sbb-alias>
        
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>
                    org.mobicents.examples.media.AnnouncementSbb
                </sbb-abstract-class-name>
                <cmp-field>
                    <cmp-field-name>announcementEndpoint</cmp-field-name>
                </cmp-field>
                <cmp-field>
                    <cmp-field-name>index</cmp-field-name>
                </cmp-field>
                <cmp-field>
                    <cmp-field-name>sequence</cmp-field-name>
                </cmp-field>
                <cmp-field>
                    <cmp-field-name>keepAlive</cmp-field-name>
                </cmp-field>
            </sbb-abstract-class> 
            <sbb-local-interface>
                <sbb-local-interface-name>
                    org.mobicents.examples.media.Announcement
                </sbb-local-interface-name>
            </sbb-local-interface>
        </sbb-classes>
        
        
        <event event-direction="Receive" initial-event="False">
            <event-name>LinkCreated</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.media.JOIN_COMPLETE</event-type-name>
                <event-type-vendor>org.mobicents.media</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>    
        
        <!-- event event-direction="Receive" initial-event="False">
            <event-name>LinkFailed</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.media.JOIN_FAIL</event-type-name>
                <event-type-vendor>org.mobicents.media</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event --> 
        
        <event event-direction="FireAndReceive" initial-event="False">
            <event-name>LinkReleased</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.media.LINK_DROPPED</event-type-name>
                <event-type-vendor>org.mobicents.media</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>    
        
        <event event-direction="Receive" initial-event="False">
            <event-name>AnnouncementComplete</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.media.announcement.COMPLETE</event-type-name>
                <event-type-vendor>org.mobicents.media</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>    
        
        <resource-adaptor-type-binding>
            <resource-adaptor-type-ref>
                <resource-adaptor-type-name>media_ratype</resource-adaptor-type-name>
                <resource-adaptor-type-vendor>org.mobicents.media</resource-adaptor-type-vendor>
                <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
            </resource-adaptor-type-ref>
            <activity-context-interface-factory-name>slee/resources/media/1.0/acifactory</activity-context-interface-factory-name>
            <resource-adaptor-entity-binding>
                <resource-adaptor-object-name>slee/resources/media/1.0/provider</resource-adaptor-object-name>
                <resource-adaptor-entity-link>LocalMs</resource-adaptor-entity-link>
            </resource-adaptor-entity-binding>
        </resource-adaptor-type-binding>				        
    </sbb>  
    
    <sbb id="LoopbackSbb">
        <description>Represents the loopback example</description>
        <sbb-name>LoopbackSbb</sbb-name>
        <sbb-vendor>org.mobicents</sbb-vendor>
        <sbb-version>1.0</sbb-version>
        <sbb-alias>LoopbackSbb</sbb-alias>
        
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>
                    org.mobicents.examples.media.loop.LoopbackSbb
                </sbb-abstract-class-name>
            </sbb-abstract-class>                                               
            <sbb-local-interface>
                <sbb-local-interface-name>
                    org.mobicents.examples.media.loop.Conversation
                </sbb-local-interface-name>
            </sbb-local-interface>
        </sbb-classes>
        
        <event event-direction="Receive" initial-event="False">
            <event-name>LinkCreated</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.media.JOIN_COMPLETE</event-type-name>
                <event-type-vendor>org.mobicents.media</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>    
        
        <event event-direction="Receive" initial-event="False">
            <event-name>LinkFailed</event-name>
            <event-type-ref>
                <event-type-name>org.mobicents.slee.media.JOIN_FAIL</event-type-name>
                <event-type-vendor>org.mobicents.media</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>    
        
        <resource-adaptor-type-binding>
            <resource-adaptor-type-ref>
                <resource-adaptor-type-name>media_ratype</resource-adaptor-type-name>
                <resource-adaptor-type-vendor>org.mobicents.media</resource-adaptor-type-vendor>
                <resource-adaptor-type-version>1.0</resource-adaptor-type-version>
            </resource-adaptor-type-ref>
            <activity-context-interface-factory-name>slee/resources/media/1.0/acifactory</activity-context-interface-factory-name>
            <resource-adaptor-entity-binding>
                <resource-adaptor-object-name>slee/resources/media/1.0/provider</resource-adaptor-object-name>
                <resource-adaptor-entity-link>LocalMs</resource-adaptor-entity-link>
            </resource-adaptor-entity-binding>
        </resource-adaptor-type-binding>				        
    </sbb>  
    
</sbb-jar>

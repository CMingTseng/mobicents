<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE service-xml PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Service 1.0//EN"
                             "http://java.sun.com/dtd/slee-service_1_0.dtd">
<service-xml>
    <service>
        <description>Main Controller Service for Call Forward Block</description>
        <service-name>Controller Service</service-name>
        <service-vendor>Mobicents</service-vendor>
        <service-version>1.0</service-version>
        <root-sbb>
            <description>Call Controller for Call Forward and Block services</description>
            <sbb-name>CallController</sbb-name>
            <sbb-vendor>Mobicents</sbb-vendor>
            <sbb-version>1.0</sbb-version>
        </root-sbb>
        <default-priority>0</default-priority>
    </service>
    <service>
    	<description>GoodBye Service terminates an active dialog between two parties</description>
    	<service-name>GoodBye Service</service-name>
    	<service-vendor>Mobicents</service-vendor>
    	<service-version>1.0</service-version>
    	<root-sbb>
    		<description>Bye SBB</description>
        	<sbb-name>Bye SBB</sbb-name>
        	<sbb-vendor>Mobicents</sbb-vendor>
        	<sbb-version>1.0</sbb-version>
    	</root-sbb>
    	<default-priority>0</default-priority>
    </service>
</service-xml>

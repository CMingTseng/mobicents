<?xml version="1.0" encoding="utf-8"?>
<project name="XCAPClientResourceAdaptor" default="build-and-deploy-ra">

	<property name="mobicents.home" value="${ant.file.AsteriskResourceAdaptor}/../../../core" />	
	<!--import file="${mobicents.home}/xml/ant/mobicents-ant-common-properties.xml" /-->
	<import file="${mobicents.home}/xml/ant/mobicents-ant-common-ra.xml" />

	<property name="ra.name" value="xcapclient" />
	<property name="ra.entity" value="XCAPClientRA" />
	<property name="ra.id" value="${ant.project.name}#org.openxdm#1.0" />	
	
	<path id="ra.classpath">
		<path refid="mobicents.core.classpath" />
		<path refid="mobicents.ra.xcapclientra.classpath" />
	</path>

	<fileset id="ra.libs.ratype" dir="lib">
		<include name="activation.jar" />
		<include name="commons-codec-1.3.jar" />
		<include name="commons-httpclient-3.0.1.jar" />
		<include name="jaxb-api.jar" />
		<include name="jaxb-impl.jar" />
		<include name="jsr173_1.0_api.jar" />
		<include name="openxdm-xcap-client.jar" />
		<include name="openxdm-xcap-commons.jar" />
	</fileset>
	
</project>


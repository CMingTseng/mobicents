<?xml version="1.0" encoding="utf-8"?>
<project name="Release_build" default="deploy-all">

  <property name="home" value="${ant.file.Release_build}/../" />
  <property name="jboss.dir.name" value="jboss-4.2.2.GA" />
  <property name="jboss.home" value="${home}/../../${jboss.dir.name}" />
  <property name="deploy.dir" value="${jboss.home}/server/default/deploy/" />
	
  <target name="deploy-all-jmx">
    <!--ant antfile="${home}../../resources/sip/build.xml" target="deploy-jmx" inheritAll="false" /-->
    <!--ant antfile="${home}../../examples/sip-services/build.xml" target="jar-with-invite-not-initial" inheritAll="false" /-->
    <ant antfile="${home}../../examples/sip-services/build.xml" target="deploy-all-jmx" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <ant antfile="${home}../../resources/media/build.xml" target="deploy-jmx" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <ant antfile="${home}../../examples/call-controller2/du-management.xml" target="deploy-jmx" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
  </target>

  <target name="undeploy-all-jmx">
    <ant antfile="${home}../../examples/call-controller2/du-management.xml" target="undeploy-jmx" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <ant antfile="${home}../../resources/media/build.xml" target="undeploy-jmx" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <ant antfile="${home}../../examples/sip-services/build.xml" target="undeploy-all-jmx" inheritall="false" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <!--ant antfile="${home}../../resources/sip/build.xml" target="undeploy-jmx" inheritAll="false" /-->
  </target>

  <target name="deploy-all">
    <!--ant antfile="${home}../../resources/sip/build.xml" target="deploy" inheritAll="false" /-->
    <!--ant antfile="${home}../../examples/sip-services/build.xml" target="jar-with-invite-not-initial" inheritAll="false" /-->
    <ant antfile="${home}../../examples/sip-services/build.xml" target="deploy-all" inheritall="false" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <ant antfile="${home}../../resources/media/build.xml" target="deploy" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <ant antfile="${home}../../examples/call-controller2/du-management.xml" target="deploy" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
  </target>

  <target name="undeploy-all">
    <ant antfile="${home}../../examples/call-controller2/du-management.xml" target="undeploy" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <ant antfile="${home}../../resources/media/build.xml" target="undeploy" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <ant antfile="${home}../../examples/sip-services/build.xml" target="undeploy-all" inheritall="false" inheritAll="false" ><property name="jboss.home" value="${jboss.home}"></property></ant>
    <!--ant antfile="${home}../../resources/sip/build.xml" target="undeploy" inheritAll="false" /-->
  </target>

</project>

<?xml version="1.0"?>

<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.0//EN" "http://java.sun.com/dtd/slee-sbb-jar_1_0.dtd">

<sbb-jar>
	<sbb id="mgcp">
		<description>MGCP Controller</description>
		<sbb-name>MgcpSbb</sbb-name>
		<sbb-vendor>org.mobicents</sbb-vendor>
		<sbb-version>1.0</sbb-version>
		<sbb-alias>MgcpSbb</sbb-alias>

		<sbb-ref>
			<sbb-name>EndpointConfigurationSbb</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>EndpointConfigurationSbb</sbb-alias>
		</sbb-ref>

		<sbb-ref>
			<sbb-name>CreateConnectionSbb</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>CreateConnectionSbb</sbb-alias>
		</sbb-ref>

		<sbb-ref>
			<sbb-name>DeleteConnectionSbb</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>DeleteConnectionSbb</sbb-alias>
		</sbb-ref>

		<sbb-ref>
			<sbb-name>ModifyConnectionSbb</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>ModifyConnectionSbb</sbb-alias>
		</sbb-ref>

		<sbb-ref>
			<sbb-name>NotificationRequestSbb</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>1.0</sbb-version>
			<sbb-alias>NotificationRequestSbb</sbb-alias>
		</sbb-ref>

		<sbb-classes>
			<sbb-abstract-class>
				<sbb-abstract-class-name>
					org.mobicents.media.server.control.mgcp.MgcpSbb
				</sbb-abstract-class-name>
				<get-child-relation-method>
					<sbb-alias-ref>
						EndpointConfigurationSbb
					</sbb-alias-ref>
					<get-child-relation-method-name>
						getEndpointConfigurationSbbChild
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>
				<get-child-relation-method>
					<sbb-alias-ref>CreateConnectionSbb</sbb-alias-ref>
					<get-child-relation-method-name>
						getCreateConnectionSbbChild
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>
				<get-child-relation-method>
					<sbb-alias-ref>DeleteConnectionSbb</sbb-alias-ref>
					<get-child-relation-method-name>
						getDeleteConnectionSbbChild
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>
				<get-child-relation-method>
					<sbb-alias-ref>ModifyConnectionSbb</sbb-alias-ref>
					<get-child-relation-method-name>
						getModifyConnectionSbbChild
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>
				<get-child-relation-method>
					<sbb-alias-ref>
						NotificationRequestSbb
					</sbb-alias-ref>
					<get-child-relation-method-name>
						getNotificationRequestSbbChild
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>
			</sbb-abstract-class>
		</sbb-classes>

		<event event-direction="Receive" initial-event="True">
			<event-name>CreateConnection</event-name>
			<event-type-ref>
				<event-type-name>
					net.java.slee.resource.mgcp.CREATE_CONNECTION
				</event-type-name>
				<event-type-vendor>net.java</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<event event-direction="Receive" initial-event="True">
			<event-name>DeleteConnection</event-name>
			<event-type-ref>
				<event-type-name>
					net.java.slee.resource.mgcp.DELETE_CONNECTION
				</event-type-name>
				<event-type-vendor>net.java</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<event event-direction="Receive" initial-event="True">
			<event-name>ModifyConnection</event-name>
			<event-type-ref>
				<event-type-name>
					net.java.slee.resource.mgcp.MODIFY_CONNECTION
				</event-type-name>
				<event-type-vendor>net.java</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<event event-direction="Receive" initial-event="True">
			<event-name>NotificationRequest</event-name>
			<event-type-ref>
				<event-type-name>
					net.java.slee.resource.mgcp.NOTIFICATIONREQUEST
				</event-type-name>
				<event-type-vendor>net.java</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>
					jain-mgcp
				</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>
					net.java
				</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>
					2.0
				</resource-adaptor-type-version>
			</resource-adaptor-type-ref>
			<activity-context-interface-factory-name>
				slee/resources/jainmgcp/2.0/acifactory
			</activity-context-interface-factory-name>
			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					slee/resources/jainmgcp/2.0/provider
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					MGCPRA
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>
		</resource-adaptor-type-binding>
	</sbb>
</sbb-jar>

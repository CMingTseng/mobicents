<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.0//EN"
                         "http://java.sun.com/dtd/slee-sbb-jar_1_0.dtd">
<sbb-jar>
    <sbb>
        <description/>
        <sbb-name>Base_Call_Duration_SBB</sbb-name>
        <sbb-vendor>PTIn</sbb-vendor>
        <sbb-version>0.1</sbb-version>
        <profile-spec-ref>
            <profile-spec-name>Duration_Profile_CMP</profile-spec-name>
            <profile-spec-vendor>PTIn</profile-spec-vendor>
            <profile-spec-version>0.1</profile-spec-version>
            <profile-spec-alias>DurationProfile</profile-spec-alias>
        </profile-spec-ref>        
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>com.ptin.xmas.app.BaseCallDurationSbb</sbb-abstract-class-name>
                <get-profile-cmp-method>
                    <profile-spec-alias-ref>DurationProfile</profile-spec-alias-ref>
                    <get-profile-cmp-method-name>getDurationProfileCMP</get-profile-cmp-method-name>
                </get-profile-cmp-method>
                <cmp-field>
                	<cmp-field-name>inCallsList</cmp-field-name>
                </cmp-field>
                <cmp-field>
                	<cmp-field-name>timerId</cmp-field-name>
                </cmp-field>
                <cmp-field>
                	<cmp-field-name>callId</cmp-field-name>
                </cmp-field>                                
            </sbb-abstract-class>
        </sbb-classes>
        <event event-direction="Receive" initial-event="True">
            <event-name>ByeEvent</event-name>
            <event-type-ref>
                <event-type-name>ByeEvent</event-type-name>
                <event-type-vendor>PTIn</event-type-vendor>
                <event-type-version>0.1</event-type-version>
            </event-type-ref>
            <initial-event-select variable="ActivityContext"/>
        </event>
        <event event-direction="Receive" initial-event="True">
            <event-name>CallEstablishedEvent</event-name>
            <event-type-ref>
                <event-type-name>CallEstablishedEvent</event-type-name>
                <event-type-vendor>PTIn</event-type-vendor>
                <event-type-version>0.1</event-type-version>
            </event-type-ref>
            <initial-event-select variable="ActivityContext"/>
        </event>
        <event event-direction="Receive" initial-event="False">
            <event-name>TimerEvent</event-name>
            <event-type-ref>
                <event-type-name>javax.slee.facilities.TimerEvent</event-type-name>
                <event-type-vendor>javax.slee</event-type-vendor>
                <event-type-version>1.0</event-type-version>
            </event-type-ref>
        </event>        
    </sbb>
</sbb-jar>
